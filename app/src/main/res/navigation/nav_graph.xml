<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/carFragment">

    <fragment
        android:id="@+id/carFragment"
        android:name="com.example.dnmotors.carFragment.CarFragment"
        android:label="Intents"
        tools:layout="@layout/fragment_car" />

    <fragment
        android:id="@+id/favouritesFragment"
        android:name="com.example.dnmotors.favouritesFragment.FavouritesFragment"
        android:label="Service"
        tools:layout="@layout/fragment_favourites" />

    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.dnmotors.searchFragment.SearchFragment"
        android:label="Provider"
        tools:layout="@layout/fragment_search" />

    <fragment
        android:id="@+id/messagesFragment"
        android:name="com.example.dnmotors.messagesFragment.MessagesFragment"
        android:label="Receiver"
        tools:layout="@layout/fragment_messages" />

    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.dnmotors.profileFragment.ProfileFragment"
        android:label="Provider"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.example.dnmotors.settingsFragment.SettingsFragment"
        android:label="Settings"
        tools:layout="@layout/fragment_settings" />



</navigation>
